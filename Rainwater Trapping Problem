def rainwaterProb_On2(arr):
    res = 0
    for i in range(1,len(arr)-1):
        lmax = arr[i]
        for j in range(0,i):
            lmax = max(lmax , arr[j])
        rmax = arr[i]
        for j in range(i+1 , len(arr)):
            rmax = max(rmax , arr[j])
        res += min(lmax , rmax) - arr[i]
    return res
